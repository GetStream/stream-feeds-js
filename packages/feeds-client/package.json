{
  "name": "@stream-io/feeds-client",
  "version": "0.3.12",
  "packageManager": "yarn@3.2.4",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "react-native": {
        "import": "./dist/es/index.mjs",
        "require": "./dist/cjs/index.js"
      },
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js",
      "default": "./dist/es/index.mjs"
    },
    "./react-bindings": {
      "types": "./dist/types/bindings/react/index.d.ts",
      "react-native": {
        "import": "./dist/es/react-bindings.mjs",
        "require": "./dist/cjs/react-bindings.js"
      },
      "import": "./dist/es/react-bindings.mjs",
      "require": "./dist/cjs/react-bindings.js",
      "default": "./dist/es/react-bindings.mjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "license": "See license in LICENSE",
  "scripts": {
    "clean": "rimraf dist",
    "start": "vite build --watch",
    "build": "vite build",
    "test": "vitest",
    "test:unit": "vitest --exclude '__integration-tests__/**'",
    "test-ci": "vitest --exclude '__integration-tests__/docs-snippets/**' --coverage",
    "test-docs-snippets": "vitest __integration-tests__/docs-snippets/**"
  },
  "files": [
    "dist",
    "src",
    "!src/**/*.test.ts",
    "react-bindings.js",
    "react-bindings.mjs",
    "react-bindings.d.ts",
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@stream-io/logger": "^2.0.0",
    "@stream-io/state-store": "^1.1.6",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@stream-io/node-sdk": "0.6.0",
    "@types/react": "^19.1.8",
    "@vitest/coverage-v8": "3.2.4",
    "dotenv": "^16.4.5",
    "human-id": "^4.1.1",
    "react": "19.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}

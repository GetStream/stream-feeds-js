{
  "name": "@stream-io/feeds-client",
  "version": "0.1.0-0",
  "packageManager": "yarn@3.2.4",
  "main": "./dist/index.node.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": {
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "browser": {
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "node": {
        "import": "./dist/index.node.js",
        "require": "./dist/index.node.cjs"
      },
      "default": "./dist/index.node.js"
    },
    "./react-bindings": {
      "types": "./dist/@react-bindings/index.d.ts",
      "react-native": {
        "import": "./dist/index-react-bindings.browser.js",
        "require": "./dist/index-react-bindings.browser.cjs"
      },
      "browser": {
        "import": "./dist/index-react-bindings.browser.js",
        "require": "./dist/index-react-bindings.browser.cjs"
      },
      "node": {
        "import": "./dist/index-react-bindings.node.js",
        "require": "./dist/index-react-bindings.node.cjs"
      },
      "default": "./dist/index-react-bindings.node.js"
    }
  },
  "types": "./dist/index.d.ts",
  "license": "See license in LICENSE",
  "scripts": {
    "clean": "rimraf dist",
    "start": "rollup -w -c",
    "build": "yarn clean && rollup -c",
    "test": "vitest",
    "test-ci": "vitest --coverage",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.0",
    "@vitest/browser": "^2.1.3",
    "@vitest/coverage-v8": "^2.1.2",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "playwright": "^1.48.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "typescript": "^5.8.3",
    "uuid": "^11.0.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "vitest-mock-extended": "^2.0.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "react": "19.0.0"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
